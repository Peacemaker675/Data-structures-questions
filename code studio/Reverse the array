#include <bits/stdc++.h>
using namespace std;
void reverseArray(vector<int> &arr, int m) {
  int n = arr.size();
  for (int i = m + 1; i < n; i++) {
    if (i > (m + n) / 2)
      break;
    if (i > m) {
      swap(arr[i], arr[n + m - i]);
    }
  }
}

// reverse an array question 
#include<bits/stdc++.h>
vector<int> reverseArray(int n, vector<int> &nums)
{
    int start =0;
    int end =n-1;
    while(start<end){
        swap(nums[start],nums[end]);
        start++;
        end--;
    }
    return nums;
}
//using reccursive approach 

